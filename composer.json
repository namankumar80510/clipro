{
  "name": "dikki/clipro",
  "description": "A simple framework to build CLI applications with PHP",
  "type": "project",
  "homepage": "https://github.com/namankumar80510/clipro",
  "config": {
    "optimize-autoloader": true,
    "preferred-install": "dist",
    "sort-packages": true
  },
  "autoload": {
    "psr-4": {
      "App\\": "src/"
    },
    "files": [
      "src/Functions.php"
    ]
  },
  "require": {
    "dikki/clipro-core": "^0.0.2"
  }
}
